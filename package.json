{
  "name": "load-calculator",
  "version": "1.0.0",
  "description": "Professional electrical load calculator for NEC-compliant residential calculations",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "node server.js",
    "setup": "node scripts/setup-dev.js",
    "setup:dev": "node setup-dev-env.js",
    "validate-env": "node scripts/validate-env.js",
    "server:dev": "node server-dev.js",
    "test:api": "node test-api-keys.js",
    "test:solar": "node test-solar-api.js",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "build:vite": "vite build",
    "build:vercel": "vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:load-calculator": "cypress run --spec 'cypress/e2e/load-calculator/**/*.cy.ts'",
    "test:e2e:sld": "cypress run --spec 'cypress/e2e/sld/**/*.cy.ts'",
    "test:e2e:project-manager": "cypress run --spec 'cypress/e2e/project-manager/**/*.cy.ts'",
    "test:e2e:compliance": "cypress run --spec 'cypress/e2e/compliance/**/*.cy.ts'",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build --build-arg SUPABASE_URL=\"$SUPABASE_URL\" --build-arg SUPABASE_ANON_KEY=\"$SUPABASE_ANON_KEY\" --build-arg GOOGLE_MAPS_API_KEY=\"$GOOGLE_MAPS_API_KEY\" --build-arg USE_REAL_AERIAL_DATA=\"$USE_REAL_AERIAL_DATA\" --build-arg AERIAL_PROVIDER=\"$AERIAL_PROVIDER\" -t load-calculator .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t load-calculator:dev .",
    "docker:build:fullstack": "docker build -f Dockerfile.fullstack -t load-calculator:fullstack .",
    "docker:run": "docker run -p 3000:3000 load-calculator",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app -v /app/node_modules load-calculator:dev",
    "docker:run:fullstack": "docker run -p 3000:3000 -p 3001:3001 -e GOOGLE_MAPS_API_KEY=$GOOGLE_MAPS_API_KEY load-calculator:fullstack",
    "docker:compose:dev": "docker-compose --profile dev up --build",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:down:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:prod": "docker-compose -f docker-compose.prod.yml logs -f",
    "dev:docker": "docker-compose --profile fullstack up -d",
    "docker:stop": "docker-compose down",
    "docker:rebuild": "docker-compose down && docker-compose --profile fullstack up -d --build",
    "docker:health": "curl -f http://localhost:3001/api/health || echo 'Backend not responding'",
    "vercel:test": "NODE_ENV=production npm ci && npm run build",
    "vercel:dev": "vercel dev",
    "vercel:build": "vercel build",
    "vercel:build:prod": "vercel build --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-backend-cpu": "^4.15.0",
    "@tensorflow/tfjs-backend-webgl": "^4.15.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/svg.js": "^0.0.17",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "firebase": "^11.10.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.294.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.3",
    "stripe": "^14.18.0",
    "svg.js": "^2.7.1",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.19"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.45.0",
    "@rollup/rollup-linux-x64-musl": "^4.45.0",
    "@rollup/rollup-win32-x64-msvc": "^4.45.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^24.0.13",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/ui": "^1.0.0",
    "cypress": "^14.5.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "rollup": "^4.45.1",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vercel": "^44.4.3",
    "vitest": "^1.0.0"
  }
}
