{
  "version": 2,
  "buildCommand": "npm ci && npx vite build --mode production",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm ci",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VITE_NODE_ENV": "production"
    }
  }
}